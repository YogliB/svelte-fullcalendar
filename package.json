{
	"dependencies": {
		"@fullcalendar/core": "4.3.1",
		"fast-deep-equal": "2.0.1"
	},
	"description": "A Svelte component wrapper around FullCalendar",
	"devDependencies": {
		"@fullcalendar/daygrid": "4.3.0",
		"@fullcalendar/interaction": "4.3.0",
		"@fullcalendar/timegrid": "4.3.0",
		"@testing-library/cypress": "5.0.2",
		"cypress": "3.7.0",
		"eslint": "6.4.0",
		"eslint-config-prettier": "6.5.0",
		"eslint-plugin-cypress": "2.7.0",
		"eslint-plugin-svelte3": "2.7.3",
		"husky": "3.0.9",
		"lint-staged": "9.4.2",
		"prettier": "1.18.2",
		"rollup": "1.27.14",
		"rollup-plugin-commonjs": "10.1.0",
		"rollup-plugin-livereload": "1.0.4",
		"rollup-plugin-node-resolve": "5.2.0",
		"rollup-plugin-postcss": "2.0.3",
		"rollup-plugin-scss": "1.0.2",
		"rollup-plugin-svelte": "5.1.1",
		"rollup-plugin-terser": "5.1.2",
		"sirv-cli": "0.4.4",
		"start-server-and-test": "1.10.6",
		"svelte": "3.12.1"
	},
	"files": [
		"src",
		"dist/index.min.mjs",
		"dist/index.min.js"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"keywords": [
		"svelte"
	],
	"license": "MIT",
	"lint-staged": {
		"*.{html, css, scss, stylus, js, ts, json, yml, md}": [
			"prettier --write",
			"git add"
		]
	},
	"main": "dist/index.min.js",
	"module": "dist/index.min.mjs",
	"name": "svelte-fullcalendar",
	"repository": "https://github.com/YogliB/svelte-fullcalendar",
	"scripts": {
		"build": "rollup -c",
		"cy:open": "cypress open",
		"cy:run": "cypress run",
		"dev": "rollup -c -w",
		"lint": "eslint --color --ignore-path .gitignore .",
		"prepublishOnly": "npm run build",
		"start": "sirv public",
		"test": "start-server-and-test dev http://localhost:5000 cy:run"
	},
	"svelte": "src/components/FullCalendar.svelte",
	"version": "0.0.4"
}
