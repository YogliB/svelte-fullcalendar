{
	"dependencies": {
		"@fullcalendar/core": "4.3.1",
		"fast-deep-equal": "2.0.1"
	},
	"description": "A Svelte component wrapper around FullCalendar",
	"devDependencies": {
		"@fullcalendar/daygrid": "4.3.0",
		"@fullcalendar/interaction": "4.3.0",
		"@fullcalendar/timegrid": "4.3.0",
		"eslint": "6.4.0",
		"eslint-config-prettier": "6.2.0",
		"eslint-plugin-jest": "22.17.0",
		"eslint-plugin-svelte3": "2.7.3",
		"husky": "3.0.5",
		"lint-staged": "9.2.5",
		"npm-run-all": "4.1.5",
		"prettier": "1.18.2",
		"rollup": "1.21.2",
		"rollup-plugin-commonjs": "10.1.0",
		"rollup-plugin-livereload": "1.0.1",
		"rollup-plugin-node-resolve": "5.2.0",
		"rollup-plugin-postcss": "2.0.3",
		"rollup-plugin-scss": "1.0.2",
		"rollup-plugin-svelte": "5.1.0",
		"rollup-plugin-terser": "5.1.1",
		"sirv-cli": "0.4.4",
		"svelte": "3.12.0"
	},
	"files": [
		"src",
		"dist/index.min.mjs",
		"dist/index.min.js"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"keywords": [
		"svelte"
	],
	"license": "MIT",
	"lint-staged": {
		"*.{html, css, scss, stylus, js, ts, json, yml, md}": [
			"prettier --write",
			"git add"
		]
	},
	"main": "dist/index.min.js",
	"module": "dist/index.min.mjs",
	"name": "svelte-fullcalendar",
	"svelte": "src/components/FullCalendar.svelte",
	"repository": "https://github.com/YogliB/svelte-fullcalendar",
	"scripts": {
		"lint": "eslint --color --ignore-path .gitignore .",
		"test": "jest --verbose",
		"autobuild": "rollup -c -w",
		"build": "rollup -c",
		"dev": "run-p start:dev autobuild",
		"prepublishOnly": "npm run build",
		"start:dev": "sirv public --single --dev"
	},
	"version": "0.0.4"
}
